#include<bits/stdc++.h>
using namespace std;
int main(){
	short int n;
	long long min = 10e9 + 1 , max = -1, x;
	int t;
	cin >> t;
	while(t--){
		cin >> n;
		while(n--){
			cin >> x;
			if(x>max)
				max = x;
			if(x<min)
				min = x;
		}
		cout << max-min << endl;
		min = 10e9 + 1 ;
		max = -1;
	}
}
